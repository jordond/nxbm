{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/ts-node/dist/bin.js",
      "args": ["--files", "--no-cache", "${workspaceRoot}/src/index.ts", "--level=debu"],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector"
    },
    {
      "name": "Server-dist",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/dist/index.js",
      "cwd": "${workspaceRoot}",
      "protocol": "inspector"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229
    }
  ]
}